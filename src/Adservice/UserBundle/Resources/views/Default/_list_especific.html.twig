{% for user in users %}
    <tr>
        <td>{{ user.id }}</td>
        <td><a id="list_username">{{ user.username }}</a> 
            {% if role == 'admin'      %} <span class="label label-primary">{% trans %} Admin    {% endtrans %}</span> {% endif %}
            {% if role == 'assessor'   %} <span class="label label-success">{% trans %} Assessor {% endtrans %}</span> {% endif %}
            {% if role == 'ad'         %} <span class="label label-warning">{% trans %} AD       {% endtrans %}</span> {% endif %}
            {% if user.active == false %} <span class="label label-danger"> {% trans %} Inactive {% endtrans %}</span> {% endif %}</td>
        <td>{{ user.name }}</td>
        <td>{{ user.surname}}</td>
        <td>{{ user.email1 }}</td>
        <td>{{ user.phoneNumber1 }}</td>
        <td>
            <a id="btn_edit{{ user.username }}"   href="{{ path("user_edit", { 'id': user.id }) }}"><span class="glyphicon glyphicon-pencil"></span></a>
            <a id="btn_delete{{ user.username }}" href="#"><span class="glyphicon glyphicon-trash" data-toggle="modal" data-target="#myModal" data-id="{{ user.id }}"></span></a>
        </td>
    </tr>
{% endfor %}