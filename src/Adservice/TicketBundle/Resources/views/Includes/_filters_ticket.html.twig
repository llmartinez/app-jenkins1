
    {% if workshop.id is defined and workshop.id != "" %}

        {# New Ticket #}
        {% if workshop.active == 1 %}

            {% if is_granted('ROLE_SUPER_ADMIN')%}
                <a class="btn btn-primary mb15" id="newTicket" href="{{ path('newTicket', { 'id_workshop' : workshop.id }) }}"><img class="img_icon" src="{{ asset('bundles/util/images/icon/ticket_new.png') }}"> {% trans %} new.ticket {% endtrans %}</a>
            {% elseif workshop.country == app.security.getToken().getUser().getCountry() %}
                <a class="btn btn-primary mb15" id="newTicket" href="{{ path('newTicket', { 'id_workshop' : workshop.id }) }}"><img class="img_icon" src="{{ asset('bundles/util/images/icon/ticket_new.png') }}"> {% trans %} new.ticket {% endtrans %}</a>
            {% endif %}

        {% else %}
            {# <a class="btn" id="newTicket" title="{% trans %} workshop.inactive {% endtrans %}" alt="{% trans %} workshop.inactive {% endtrans %}"><img class="img_icon" src="{{ asset('bundles/util/images/icon/ticket_new.png') }}"> {% trans %} new.ticket {% endtrans %}</a> #}
        {% endif %}
            <input type="hidden" id="id_workshop" name="id_workshop" value="{{ workshop.id }}">
    {% else  %}

        <form action="{{ path('findTicketById') }}" id="findTicketById" class="form" role="form" method="post" enctype="multipart/form-data">

            <p>{% trans %} ticket.flt.searchBy.id {% endtrans %}:</p>
            <div class="col-xs-8 col-md-8 form-group pdl0">
                <input type='text' class="form-control" id='flt_id' name='flt_id' placeholder='{% trans %} id {% endtrans %}'>
            </div>
            <div class="col-xs-4 col-md-4 form-group pdl0">
                    <button type="submit" class="btn btn-primary" id="btn_search" name="btn_search">
                        <img class="img_icon" src="{{ asset('bundles/util/images/icon/search_btn.png') }}">  {% trans %} btn.search {% endtrans %}</button>
            </div>
        </form>

        <form id="contact" class="form" role="form" method="post" enctype="multipart/form-data">

            <div class="col-xs-12 col-md-12 form-group pdl0">{% trans %} ticket.flt.searchBy.car {% endtrans %}: </div>

            <div class="col-xs-8 col-md-8 form-group  pdl0">
                <label for="new_car_form_brand">{% trans %} brand {% endtrans %}</label>
                <select id="new_car_form_brand" name="new_car_form_brand" class="form-control h65" multiple="multiple", required>
                    {% for brand in brands %}
                        <option value={{ brand.id }}>{{ brand.name }}</option>
                    {% endfor %}
                </select>
            </div>

            <div class="col-xs-8 col-md-8 form-group  pdl0">
                <label for="new_car_form_model">{% trans %} model {% endtrans %}</label>
                <select id="new_car_form_model" name="new_car_form_model" class="form-control h65" multiple="multiple", required></select>
            </div>

            <div class="col-xs-8 col-md-8 form-group  pdl0">
                <label for="new_car_form_version">{% trans %} version {% endtrans %}</label>
                <select id="new_car_form_version" name="new_car_form_version" class="form-control h65" multiple="multiple" >
                </select>
            </div>

            <div class="col-xs-12 col-md-12 form-group pdl0">
                <div class="col-xs-8 col-md-8 form-group pdl0"></div>
                <div class="col-xs-3 col-md-3 form-group pdl0">
                        <button type="button" class="btn btn-primary" id="btn_search_by_bmv" name="btn_search_by_bmv">
                            <img class="img_icon" src="{{ asset('bundles/util/images/icon/search_btn.png') }}">  {% trans %} btn.search {% endtrans %}</button>
                        <input type="hidden" id="alert" value="{% trans %} error.bad_introduction {% endtrans %}">
                </div>
            </div>
        </form>
        <br>

        <div class="col-xs-12 col-md-12 form-group pdl0">{% trans %} ticket.history.check {% endtrans %}:</div>
    {% endif %}
    {###  HISTORICO  ###}
    {% if is_granted('ROLE_ASSESSOR') %}
        <div class="col-xs-12 col-md-12 form-group pdl0">
        {% if workshop.id is defined and workshop.id != "" %}
            <button class="btn btn-primary" onclick="window.open('{{ path('list_incidences', { 'page' : 1, 'id_taller' : workshop.codeWorkshop, 'id_socio' : workshop.partner.codePartner }) }}')"><img class="img_icon" src="{{ asset('bundles/util/images/icon/historico.png') }}"> {% trans %} lock.list_incidences {% endtrans %}</button>
        {% else %}
            <button class="btn btn-primary" onclick="window.open('{{ path('list_incidences', { 'page' : 1, 'id_taller' : 'none', 'id_socio' : 'none' }) }}')"><img class="img_icon" src="{{ asset('bundles/util/images/icon/historico.png') }}"> {% trans %} lock.list_incidences {% endtrans %}</button>
        {% endif %}
        </div>
    {% endif %}