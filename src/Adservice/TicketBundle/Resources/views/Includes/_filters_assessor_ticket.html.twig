
        <form action="{{ path('findAssessorTicketByBMV', { 'page' : '1' }) }}" id="contact" class="form" role="form" method="post" enctype="multipart/form-data">

            <input type='hidden' id='ftbmv_page' name='ftbmv_page' value="{{pagination.page}}">
            {% if workshop is defined and workshop.id != "" %}
                <input type='hidden' id='ftbmv_codepartner' name='ftbmv_codepartner' value="{{workshop.partner.codepartner}}">
                <input type='hidden' id='ftbmv_codeworkshop' name='ftbmv_codeworkshop' value="{{workshop.codeworkshop}}">
                <input type='hidden' id='ftbmv_email' name='ftbmv_email' value="{{workshop.email1}}">
                <input type='hidden' id='ftbmv_phone' name='ftbmv_phone' value="{{workshop.phoneNumber1}}">
            {% endif %}

            <div class="col-xs-2 col-md-2 form-group  pdl0">
                <label for="new_car_form_brand">{% trans %} brand {% endtrans %}</label>
                <select id="new_car_form_brand" name="new_car_form_brand" class="form-control {# h65 #}" {# multiple="multiple" #} {# required #}>
                        <option value="0"></option>
                    {% for _brand in brands %}
                        <option value={{ _brand.id }} {% if brand is defined and brand == _brand.id %}selected{% endif %}>{{ _brand.name }}</option>
                    {% endfor %}
                </select>
            </div>

            <div class="col-xs-2 col-md-2 form-group  pdl0">
                <label for="new_car_form_model">{% trans %} model {% endtrans %}</label>
                <select id="new_car_form_model" name="new_car_form_model" class="form-control {# h65 #}" {# multiple="multiple" #} {# required #}>
                    {% if model.id is defined %}<option value="{{model.id}}">{{model.name}}</option>{% endif %}
                </select>
            </div>

            <div class="col-xs-2 col-md-2 form-group  pdl0">
                <label for="new_car_form_version">{% trans %} version {% endtrans %}</label>
                <select id="new_car_form_version" name="new_car_form_version" class="form-control {# h65 #}" {# multiple="multiple" #} >
                    {% if version.id is defined %}<option value="{{version.id}}">{{version.name}}</option>{% endif %}
                </select>
            </div>

            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_year">{% trans %} year {% endtrans %}</label>
                <input type="text" id="new_car_form_year" name="new_car_form_year" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if year is defined %}value="{{year}}"{% endif %}>
                </input>
            </div>

            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_motor">{% trans %} motor {% endtrans %}</label>
                <input type="text" id="new_car_form_motor" name="new_car_form_motor" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if motor is defined %}value="{{motor}}"{% endif %}>
                </input>
            </div>

            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_kw">{% trans %} kw {% endtrans %}</label>
                <input type="text" id="new_car_form_kw" name="new_car_form_kw" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if kw is defined %}value="{{kw}}"{% endif %}>
                </input>
            </div>

            <div class="col-xs-2 col-md-2 form-group pdl0">
                <label for="new_car_form_importance">{% trans %} importance {% endtrans %}</label>
                <select id="new_car_form_importance" name="new_car_form_importance" class="form-control {# h65 #}" {# multiple="multiple" #} >
                    <option value="0"></option>
                    {% for _importance in importances %}
                        <option value={{ _importance.id }} {% if importance is defined and importance == _importance.id %}selected{% endif %}>{{ _importance.importance|trans }}</option>
                    {% endfor %}
                </select>
            </div>

            <div class="col-xs-2 col-md-2 form-group pdl0">
                <label for="id_system">{% trans %} system {% endtrans %}</label>
                <select id="id_system" name="id_system" class="form-control {# h65 #}" {# multiple="multiple" #} >
                    <option value="0"></option>
                    {% for _system in systems %}
                        <option value={{ _system.id }} {% if system is defined and system == _system.id %}selected{% endif %}>{{ _system.name }}</option>
                    {% endfor %}
                </select>
            </div>

            <div class="col-xs-2 col-md-2 form-group pdl0">
                <label for="new_car_form_subsystem">{% trans %} subsystem {% endtrans %}</label>
                <select id="new_car_form_subsystem" name="new_car_form_subsystem" class="form-control {# h65 #}" {# multiple="multiple" #} >
                    {% if subsystem.id is defined %}<option value="{{subsystem.id}}">{{subsystem.name}}{% endif %}</option>
                </select>
            </div>


            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_displacement">{% trans %} displacement {% endtrans %}</label>
                <input type="text" id="new_car_form_displacement" name="new_car_form_displacement" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if displacement is defined %}value="{{displacement}}"{% endif %} >
                </input>
            </div>

            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_vin">{% trans %} vin {% endtrans %}</label>
                <input type="text" id="new_car_form_vin" name="new_car_form_vin" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if vin is defined %}value="{{vin}}"{% endif %} >
                </input>
            </div>

            <div class="col-xs-1 col-md-1 form-group  pdl0">
                <label for="new_car_form_plateNumber">{% trans %} plate_number {% endtrans %}</label>
                <input type="text" id="new_car_form_plateNumber" name="new_car_form_plateNumber" class="form-control {# h65 #}" {# multiple="multiple" #}
                    {% if plateNumber is defined %}value="{{plateNumber}}"{% endif %} >
                </input>
            </div>
            {# <div class="col-xs-1 col-md-1 form-group pdl0">
                    <label for="new_car_form_importance">{% trans %} ticket {% endtrans %} ID</label>
                    <input type='text' class="form-control" id="btn_searchbyid" placeholder='{% trans %} id {% endtrans %}'>
            </div> #}
            <div class="col-xs-2 col-md-2 form-group pdl0">
                <div class="col-xs-12 col-md-12 form-group pdl0">
                        {# <label for="new_car_form_subsystem">{% trans %} search_by_car {% endtrans %}</label> #}
                        <button type="submit" class="btn btn-primary" id="btn_search_by_bmv" name="btn_search_by_bmv">
                            <img class="img_icon" src="{{ asset('bundles/util/images/icon/search_btn.png') }}">  {% trans %} btn.search {% endtrans %}</button>
                        <input type="hidden" id="alert" value="{% trans %} error.bad_introduction {% endtrans %}">
                </div>
                </div>
            </div>
        </form>
       {#  <form action="{{ path('findTicketById') }}" id="findTicketById" class="form" role="form" method="post" enctype="multipart/form-data">
            <input type="hidden" id='flt_id' name='flt_id'>
        </form> #}
        <div class="col-xs-1 col-md-1 form-group pdl0">
       <form action="{{ path("listTicket", { 'option': 'assessor_pending'}) }}" id="clear_filters" class="form" role="form" method="post" enctype="multipart/form-data">
            {# <label for="clear_filters">{% trans %} clear_filters {% endtrans %}</label> #}
            <button type="submit" class="btn btn-primary" id="clear_filters" name="clear_filters">
                <img class="img_icon" src="{{ asset('bundles/util/images/icon/search_btn.png') }}">  {% trans %} btn.clear {% endtrans %}</button>
        </form>
        </div>
