    <legend>
    	{{ 'car'|trans }}
        
        {# editar Ticket #}
        {% if is_granted('ROLE_ASSESSOR') %}                         
            <img class="img_icon" src="{{ asset('bundles/util/images/icon/' ~ car.status|default('undefined') ~ '.png') }}" title="{{ car.status | trans }}">
            <a href="{{ path('editCar', { 'id': ticket.id}) }}">
                <img class="img_icon" src="{{ asset('bundles/util/images/icon/pencil.png') }}" title="{{ 'edit.car'|trans }}"></span>
            </a>
        {% endif %}    
    </legend>

<input type="hidden" id="id_car" name="id_car" value="{{ car.id }}">

<div class="form-group">
	<table>
		<tr><td><img class="img_icon" src="{{ asset('bundles/util/images/icon/car.png') }}"></td><td>{% if brand is defined and brand != "" %}{{ brand    }}</td>{% endif %}</tr>

		{% if car.model        != "" %}<tr><td><b>{{ 'model'|trans }}:</b> </td><td>{{ car.model   }}</td></tr>{% endif %}
		{% if car.version      != "" %}<tr><td><b>{{ 'version'|trans }}:</b> </td><td>{{ car.version }}</td></tr>{% endif %}

		{% if car.year         != "" %}<tr><td><b>{{ 'year'|trans }}</b><td>{{ car.year         }}</td></tr>{% endif %}
		{% if car.motor        != "" %}<tr><td><b>{{ 'motor'|trans }}</b><td>{{ car.motor        }}</td></tr>{% endif %}
		{% if car.kW           != "" %}<tr><td><b>{{ 'kW'}}</b><td>{{ car.kW           }}</td></tr>{% endif %}
		{% if car.displacement != "" %}<tr><td><b>{{ 'displacement'|trans }}</b><td>{{ car.displacement }}</td></tr>{% endif %}
		{% if car.vin          != "" %}<tr><td><b>{{ 'vin'|trans }}</b><td>{{ car.vin          }}</td></tr>{% endif %}

		{% if car.plateNumber  != "" %}<tr><td><b>{{ 'plate_number'|trans }}: </b><td>{{ car.plateNumber  }}</td></tr>{% endif %}
	</table>

    {% include 'TicketBundle:Includes:_ged_btns.html.twig' %}

</div>
