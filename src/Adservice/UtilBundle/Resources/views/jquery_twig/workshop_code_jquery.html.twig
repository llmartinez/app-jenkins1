
<script>

	$(document).ready(function() {
		$( "#w_idpartner" ).focus();
	});

	function codeNumeric(){
		if (!$.isNumeric($( "#w_id" ).val()) || !$.isNumeric($( "#w_idpartner" ).val())) {
			event.preventDefault();
			alert('{% trans %} error.numeric {% endtrans %}');
		}
	}

	$( "#btn_check" ).click(function() {

	  	if (($( "#w_id" ).val() == "" || $( "#w_idpartner" ).val() == "") && $( "#w_email" ).val() == '' && $( "#w_tel" ).val() == '' ) {
	  		event.preventDefault();
			$( "#w_idpartner" ).focus();
			$( "#lbl_code" ).html("{% trans %} code.partner {% endtrans %}");
	  	}else{
	  		codeNumeric();
	  	}
	});

	$( "#w_id" ).on( "keydown", function( event ) {

	  	if (event.which == 13) {
	  		if ($( "#w_idpartner" ).val() == "") {
		  		event.preventDefault();
				$( "#w_idpartner" ).focus();
				$( "#lbl_code" ).html("{% trans %} code.partner {% endtrans %}");
			}else{
	  			codeNumeric();
	  		}
	  	}
	});

	$( "#w_idpartner" ).on( "keydown", function( event ) {

		if (event.which == 13) {
		  	event.preventDefault();
		  	$( "#w_id" ).focus();
		}
	});

</script>