    <script>

        $(document).ready(function() {
            {% if country != 'none' %} $("#flt_country").val('{{ country }}');{% endif %}

        //REDIRIGE A LA PAGINA EN LA QUE SE HAYA HECHO CLICK
        $('#flt_country').click(function() {

            var country = $('#flt_country').val();

            if(country == null) country = 'none';

            {% if id_taller != null and id_taller != 'none' %}
                    var url = "{{ path('list_incidences', { 'page': 'plc_page', 'id_taller' : id_taller, 'id_socio' : id_socio, 'country': 'plc_country'}) }}";
            {% else %}
                    var url = "{{ path('list_incidences', { 'page': 'plc_page', 'id_taller' : 'none', 'id_socio' : 'none', 'country': 'plc_country'}) }}";
            {% endif %}
            url = url.replace("plc_page", 1);
            url = url.replace("plc_country", country);

            window.open(url, "_self");
        });
            //util.js
            table_filter();
        });

    </script>