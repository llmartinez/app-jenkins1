
    {# DECLARACION DE VARIABLES PARA LA PAGINACION #}
    {% set page      = pagination.page      %}
    {% set prevpages = pagination.prevpages %}
    {% set nextpages = pagination.nextpages %}
    {% set totalpag  = pagination.totalpag  %}

    {% if catserv.id is defined %} {% set id_catserv  = catserv.id %}
    {% else %} {% set id_catserv  = 0 %} {% endif %}

    {# PAGINACION #}
{% if page != 0 %}
    <input type="hidden" id="totalpag" value="{{ totalpag }}">
    <input type="hidden" id="country" value="{{ country }}">
    {% if not is_granted('ROLE_ASSESSOR') %} {% set option = 'all' %}{% endif %}
   
    {% if brand is not null or (model is defined and model is not null) or (version  is defined and version is not null) or year is defined or motor is defined or kw is defined or system is defined or importance is defined or subsystem.id is defined or displacement is defined or vin is defined or plateNumber is defined %}
        {% set findByBMV = 1 %}{% endif %}
    <div id="pagination"  class="pagination_content">

	    {# FLECHAS PAGINAS ANTERIORES #}
    	<a id="firstpage"    class="page_number" title="{{ 'page.first'|trans }}"
            {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV(1)"
            {% else                                       %}href="{{ path(route, { page: 1, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
            {% endif %}>
            <img class="img_icon" src="{{ asset('bundles/util/images/icon/arrow_left_double.png') }}">
        </a>

        {% if page-1 <= 0 %} {% set this_prev_page = 1 %} {% else %} {% set this_prev_page = page-1 %}{% endif %}

    	<a id="btn_anterior" class="page_number" title="{{ 'page.prev'|trans }}"
            {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV({{this_prev_page}})"
            {% else                                       %}href="{{ path(route, { page: this_prev_page, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
            {% endif %}>
            <img class="img_icon" src="{{ asset('bundles/util/images/icon/arrow_left.png') }}">
        </a>

	    {# PAGINAS ANTERIORES #}
    	{% if prevpages > 0 %}
	    	{% for prev_page in prevpages..1 %}

                {% set this_prev_page = page - prev_page %}

                <a class="page_number change_page" title="{{ 'page'|trans }} {{ this_prev_page }}"
                {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV({{this_prev_page}})"
                {% else                                       %}href="{{ path(route, { page: this_prev_page, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
                {% endif %}> {# , order_by: order_by, trend: trend, mail: mail }) }}"> #}
                    {{ this_prev_page }}
                </a>
	    	{% endfor %}
    	{% endif %}

    	{# PAGINA ACTUAL #}
	    <input  type="text"   id="page"  class="page_number page_selected" value="{{ page }}"  title="{{ 'page.goto'|trans }} {{ page }}" disabled >

	    {# PAGINAS SIGUIENTES #}
    	{% if nextpages > 0 %}
			{% for next_page in 1..nextpages %}

                {% set this_next_page = page + next_page %}

                <a class="page_number change_page" title="{{ 'page'|trans }} {{ this_next_page }}"
                {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV({{this_next_page}})"
                {% else                                       %}href="{{ path(route, { page: this_next_page, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
                {% endif %}>
                    {{ this_next_page }}
                </a>
	    	{% endfor %}
    	{% endif %}

	    {# FLECHAS PAGINAS SIGUIENTES #}

        {% if page+1 >= totalpag %} {% set this_next_page = totalpag %} {% else %} {% set this_next_page = page+1 %}{% endif %}

        <a id="btn_siguiente" class="page_number" title="{{ 'page.next'|trans }}"
            {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV({{this_next_page}})"
            {% else                                       %}href="{{ path(route, { page: this_next_page, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
            {% endif %}>
            <img class="img_icon" src="{{ asset('bundles/util/images/icon/arrow_right.png') }}">
        </a>
        <a id="totalpage"    class="page_number" title="{{ 'page.last'|trans }}({{ totalpag }})"
            {% if findByBMV is defined and findByBMV == 1 %}onclick="updatePageAndFindByBMV({{totalpag}})"
            {% else                                       %}href="{{ path(route, { page: totalpag, num_rows: num_rows, country: country, catserv: id_catserv, lang: lang, option: option, workshop_id: workshop_id, adviser_id: adviser_id }) }}"
            {% endif %}>
            <img class="img_icon" src="{{ asset('bundles/util/images/icon/arrow_right_double.png') }}">
        </a>
    </div>
{% endif %}