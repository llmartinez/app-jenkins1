		    	if(partner == null) partner = 'none';
		    	if(status  == null) status  = 'none';

		    	var url     = "{{ path('listStatistics', { 'type': 'workshop', 'page': 'plc_page', 'from_y': 'plc_from_y', 'from_m': 'plc_from_m', 'from_d': 'plc_from_d', 'to_y': 'plc_to_y', 'to_m': 'plc_to_m', 'to_d': 'plc_to_d', 'partner': 'plc_partner', 'status': 'plc_status' }) }}";

