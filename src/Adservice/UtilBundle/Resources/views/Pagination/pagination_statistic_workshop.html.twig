
    {# PAGINACION #}
    <div id="pagination"  class="pagination_content">

    	{# DECLARACION DE VARIABLES PARA LA PAGINACION #}
    	{% set page      = pagination.page      %}
    	{% set prevpages = pagination.prevpages %}
	    {% set nextpages = pagination.nextpages %}
	    {% set totalpag  = pagination.totalpag  %}

	    {# FLECHAS PAGINAS ANTERIORES #}
    	<button id="firstpage"    class="page_number"><span class="glyphicon glyphicon-backward" title="{% trans %} page.first {% endtrans %}"   ></span></button>
    	<button id="btn_anterior" class="page_number"><span class="glyphicon glyphicon-chevron-left" title="{% trans %} page.prev {% endtrans %}"></span></button>

	    {# PAGINAS ANTERIORES #}
    	{% if prevpages > 0 %}
	    	{% for prev_page in prevpages..1 %}
	    		<button class="page_number change_page" >{{ page - prev_page }}</button>
	    	{% endfor %}
    	{% endif %}

    	{# PAGINA ACTUAL #}
	    <input  type="text"   id="page"  class="page_number page_selected" value="{{ page }}" title="{% trans %} page.goto {% endtrans %} {{ page }}" disabled >

	    {# PAGINAS SIGUIENTES #}
    	{% if nextpages > 0 %}
			{% for next_page in 1..nextpages %}
	    		<button class="page_number change_page" >{{ page + next_page }}</button>
	    	{% endfor %}
    	{% endif %}

	    {# FLECHAS PAGINAS SIGUIENTES #}
    	<button id="btn_siguiente" class="page_number"><span class="glyphicon glyphicon-chevron-right" title="{% trans %} page.next {% endtrans %}"></span></button>
    	<button id="totalpage"     class="page_number"><span class="glyphicon glyphicon-forward" title="{% trans %} page.last {% endtrans %}({{ totalpag }})"></span></button>

    </div>

    <script type="text/javascript">

    	//REDIRIGE A LA PRIMERA PAGINA
	    $('#firstpage').click(function() {

	    	var from_y  = $('#wks_from_y').val();
	        var from_m  = $('#wks_from_m').val();
	        var from_d  = $('#wks_from_d').val();
	        var to_y    = $('#wks_to_y').val();
	        var to_m    = $('#wks_to_m').val();
	        var to_d    = $('#wks_to_d').val();
	    	var partner = $('#flt_wks_partner').val();
	    	var status  = $('#flt_wks_status').val();

	        if(from_y  == ""  ) from_y  = 'none';
	        if(from_m  == ""  ) from_m  = 'none';
	        if(from_d  == ""  ) from_d  = 'none';
	        if(to_y    == ""  ) to_y    = 'none';
	        if(to_m    == ""  ) to_m    = 'none';
	        if(to_d    == ""  ) to_d    = 'none';
	    	if(partner == null) partner = 'none';
	    	if(status  == null) status  = 'none';

	    	var url     = "{{ path('listStatistics', { 'type': 'workshop', 'page': '1', 'from_y': 'plc_from_y', 'from_m': 'plc_from_m', 'from_d': 'plc_from_d', 'to_y': 'plc_to_y', 'to_m': 'plc_to_m', 'to_d': 'plc_to_d', 'partner': 'plc_partner', 'status': 'plc_status' }) }}";

	    	url = url.replace("plc_from_y", from_y);
	        url = url.replace("plc_from_m", from_m);
	        url = url.replace("plc_from_d", from_d);
	        url = url.replace("plc_to_y", to_y);
	        url = url.replace("plc_to_m", to_m);
	        url = url.replace("plc_to_d", to_d);
	    	url = url.replace("plc_partner", partner);
	    	url = url.replace("plc_status", status);

	    	window.open(url, "_self");
	    });

