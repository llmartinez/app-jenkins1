{% extends 'TwigBundle::layout.html.twig' %}

{% block title %}
    Se ha producido un error ({{ status_code }} {{ status_text }})
{% endblock %}

{% block titleException %}
    <div id="title_error"><h1>{% trans %}error{% endtrans %}</h1></div>
{% endblock %}

{% block exception %}
    <div id="content_error">
            {% include 'TwigBundle:Exception:exception.html.twig' %}
    </div>
    <a onclick="javascript:history.back()" title="{% trans %}back_app{% endtrans %}" target="_self">{% filter upper %}{% trans %}back_app{% endtrans %}{% endfilter %}</a>
{% endblock %}