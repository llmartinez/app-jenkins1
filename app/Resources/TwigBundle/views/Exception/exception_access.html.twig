{% extends 'TwigBundle::layout.html.twig' %}

{% block title %}
    {{ 'error.access'|trans }}
{% endblock %}

{% block titleException %}
    <div id="title_error"><h1>{{ 'error.access'|trans }}</h1></div>
{% endblock %}

{% block exception %}
    <a onclick="javascript:history.back()" title="{{ 'back_app'|trans }}" target="_self">{{ 'back_app'|trans|upper }}</a>
{% endblock %}